{
  "name": "__MSG_extName__",
  "version": "1.2.5",
  "manifest_version": 2,
  "homepage_url": "https://momane.com/",
  "permissions": [
    "clipboardWrite"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://rarbg.to/torrents.php*",
        "*://rarbg.to/tv/*",
        "*://rarbg.to/catalog*",
        "*://rarbg.is/torrents.php*",
        "*://rarbg.is/tv/*",
        "*://rarbg.is/catalog*",
        "*://proxyrarbg.org/torrents.php*",
        "*://proxyrarbg.org/tv/*",
        "*://proxyrarbg.org/catalog*",
        "*://rarbgmirror.com/*",
        "*://rarbgunblock.com/*",
        "*://rarbg.link/*",
        "*://rarbgs.com/*",
        "*://rarbg.bypassed.org/*",
        "*://rarbgaccess.org/*",
        "*://rarbgmirror.xyz/*",
        "*://rarbgproxy.org/*",
        "*://rarbg.unblockall.org/*",
        "*://unblocktorrent.com/rarbg-proxy-unblock/*",
        "*://rarbg.unblocked.vc/*",
        "*://rarbg.unblocked.lol/*"
      ],
      "js": [
        "lib/jquery-3.1.0.min.js",
        "main.js"
      ],
      "css": [
        "main.css"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "logo.png",
    "16": "logo.png"
  },
  "web_accessible_resources": [
    "*.png"
  ],
  "default_locale": "en",
  "description": "__MSG_extDescription__",
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "__MSG_extName__"
  }
}